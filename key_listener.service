[Unit]
Description=key_listener
Requires=network.target telegraph_listener.service

[Service]
ExecStart=/usr/bin/python3 /usr/local/rpi_telegraph/key_listener.py
WorkingDirectory=/tmp
Restart=on-failure
RestartSec = 10
CPUSchedulingPolicy=rr
CPUSchedulingPriority=80

[Install]
WantedBy=multi-user.target
